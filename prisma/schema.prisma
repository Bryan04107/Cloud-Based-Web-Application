generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model EscapeRoom {
  id             String   @id @default(cuid())
  title          String
  timerMinutes   Int
  penaltySeconds Int
  backgroundImage String
  hotspots       String   
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}